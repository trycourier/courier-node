    "jest": "^24.8.0",
    "jest-fetch-mock": "^3.0.3",
    "prettier": "^1.18.2",
    "ts-jest": "^24.0.2",
    "tslint": "^5.18.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.5.3"
  },
  "scripts": {
    "test": "jest --config jestconfig.json",
